{
    "name": "@wizeworks/graphql-factory-mongo",
    "version": "0.1.0",
    "description": "Dynamic GraphQL API generation for MongoDB with support for metadata-driven schemas, tracing, logging, and multi-tenant relationships.",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/wize-works/graphql-factory-mongo.git"
    },
    "keywords": [
        "graphql",
        "mongodb",
        "typescript",
        "metadata",
        "multi-tenant",
        "tracing",
        "logger"
    ],
    "author": "WizeWorks",
    "license": "MIT",
    "engines": {
        "node": ">=16.0.0"
    },
    "peerDependencies": {
        "graphql": "^15.0.0 || ^16.0.0 || ^17.0.0",
        "graphql-subscriptions": "^3.0.0",
        "mongodb": ">=6.0.0 <7.0.0"
    },
    "dependencies": {
        "graphql-scalars": "^1.24.2",
        "jsonwebtoken": "^9.0.2"
    },
    "devDependencies": {
        "@semantic-release/github": "^9.0.0",
        "@semantic-release/npm": "^11.0.0",
        "@types/express": "^5.0.1",
        "express": "^5.1.0",
        "graphql-yoga": "^5.13.4",
        "semantic-release": "^21.0.0",
        "tsup": "^8.4.0",
        "typescript": "^5.8.3"
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsx examples/server/server.js",
        "clean": "rm -rf dist",
        "semantic-release": "semantic-release"
    },
    "release": {
        "branches": [
            "main"
        ],
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/npm",
            "@semantic-release/github"
        ]
    }
}
