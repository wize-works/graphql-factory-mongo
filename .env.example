# MongoDB connection URI (used in example/server.ts and tests)
MONGO_URI=mongodb://localhost:27017/wize-content

# Optional: Enable Sentry logging
SENTRY_DSN=your_sentry_dsn_here

# Semantic Release tokens (used in CI, optional for local dev)
# Only needed for local use of `semantic-release` (CI auto injects these)
GITHUB_TOKEN=your_github_token_here
NPM_TOKEN=your_npm_token_here

# Optional: Environment label
NODE_ENV=development